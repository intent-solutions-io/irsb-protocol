{
  "name": "@intentsolutionsio/irsb-sdk",
  "version": "0.1.0",
  "description": "SDK for IRSB Protocol - Intent Receipts & Solver Bonds",
  "publishConfig": {
    "access": "public"
  },
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "files": [
    "dist",
    "src"
  ],
  "scripts": {
    "build": "tsup src/index.ts --format cjs,esm --dts",
    "dev": "tsup src/index.ts --format cjs,esm --dts --watch",
    "lint": "eslint src/",
    "test": "vitest",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "ethereum",
    "intent",
    "solver",
    "erc-7683",
    "defi",
    "accountability"
  ],
  "author": "Intent Solutions IO <jeremy@intentsolutions.io>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/intent-solutions-io/irsb-protocol.git",
    "directory": "sdk"
  },
  "homepage": "https://irsb-protocol.web.app",
  "dependencies": {
    "ethers": "^6.9.0",
    "graphql": "^16.8.0",
    "graphql-request": "^6.1.0"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "tsup": "^8.0.1",
    "typescript": "^5.3.0",
    "vitest": "^1.0.0"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
