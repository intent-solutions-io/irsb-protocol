{
  "name": "irsb-x402",
  "version": "0.1.0",
  "description": "x402 HTTP Payment Protocol integration for IRSB ReceiptV2",
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "files": [
    "dist",
    "src"
  ],
  "scripts": {
    "build": "tsup src/index.ts --format esm --dts --clean",
    "dev": "tsup src/index.ts --format esm --dts --watch",
    "test": "vitest run",
    "test:watch": "vitest",
    "lint": "eslint src/ --ext .ts",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "ethers": "^6.13.0"
  },
  "devDependencies": {
    "@types/node": "^20.11.0",
    "eslint": "^8.56.0",
    "tsup": "^8.0.0",
    "typescript": "^5.3.0",
    "vitest": "^1.2.0"
  },
  "peerDependencies": {
    "ethers": "^6.0.0"
  },
  "keywords": [
    "x402",
    "irsb",
    "intent",
    "receipt",
    "payment",
    "http-402",
    "web3"
  ],
  "author": "Intent Solutions",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/intent-solutions-io/irsb-protocol.git",
    "directory": "packages/x402-irsb"
  }
}
